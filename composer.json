{
    "name": "cndrsdrmn/spreadsheet-installer",
    "description": "Composer plugin to install go-spreadsheet binary",
    "type": "composer-plugin",
    "keywords": [
        "php",
        "boilerplate",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Candra Sudirman",
            "email": "dev.cndrsdrmn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-zip": "*",
        "composer-plugin-api": "^2.0",
        "symfony/filesystem": "^7.3",
        "symfony/http-client": "^7.3",
        "symfony/process": "^7.3"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.1",
        "symfony/var-dumper": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Cndrsdrmn\\SpreadsheetInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "pint",
        "refactor": "rector",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan",
        "test:unit": "phpunit",
        "test": [
            "@test:lint",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "extra": {
        "class": "Cndrsdrmn\\SpreadsheetInstaller\\Plugin"
    }
}